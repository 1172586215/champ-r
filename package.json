    "webpack": "4.41.2",
    "webpack-dev-server": "3.9.0",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "build": {
    "appId": "com.al.champ-r",
    "files": [
      "node_modules/",
      "build/",
      "main.js",
      "package.json"
    ],
    "win": {
      "icon": "./src/assets/app-icon.ico",
      "requestedExecutionLevel": "highestAvailable"
    }
  },
  "np": {
    "publish": false,
    "releaseDraft": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{md,css,scss}": [
      "prettier --write"
    ],
    "src/*.{js}": [
      "eslint",
      "prettier --write"
    ]
  }
}
