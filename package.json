{
  "name": "champ-r",
  "version": "0.8.0",
  "license": "MIT",
  "description": "Get champion items for lol, Windows only.",
  "productName": "ChampR",
  "repository": "cangzhang/champ-r",
  "homepage": "./",
  "author": {
    "name": "Al Cheung",
    "email": "cangzan@gmail.com",
    "url": "https://github.com/cangzhang"
  },
  "main": "main.js",
  "scripts": {
    "build": "yarn build-react && yarn dist",
    "build-react": "node scripts/build.js",
    "dist": "electron-builder --win --x64",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint --fix",
    "pack": "electron-builder --dir",
    "release": "np",
    "start": "concurrently \"yarn start-react\" \"wait-on http://localhost:3000 && yarn start-electron\"",
    "start-electron": "cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true run-electron .",
    "start-react": "cross-env BROWSER=none node scripts/start.js",
    "test": "npm run lint",
    "test-react": "node scripts/test.js"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "baseui": "9.67.2",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "electron-context-menu": "0.16.0",
    "electron-debug": "3.0.1",
    "electron-is-dev": "^1.1.0",
    "electron-store": "5.1.1",
    "electron-unhandled": "^3.0.0",
    "electron-updater": "^4.0.6",
    "electron-util": "0.14.0",
    "i18next": "^19.3.4",
    "immer": "6.0.3",
    "lodash": "^4.17.15",
    "nanoid": "3.0.2",
    "normalize.css": "^8.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-feather": "^2.0.3",
    "react-i18next": "^11.3.4",
    "react-router-dom": "^5.1.2",
    "styletron-engine-atomic": "1.4.6",
    "styletron-react": "5.2.7"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@svgr/webpack": "4.3.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.5",
    "babel-preset-react-app": "^9.1.0",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "compression-webpack-plugin": "^3.1.0",
    "concurrently": "^5.0.2",
    "cross-env": "^7.0.0",
    "css-loader": "3.2.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "electron": "8.2.0",
    "electron-builder": "22.4.1",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-baseui": "9.67.2",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "file-loader": "4.3.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "4.0.0-beta.5",
    "husky": "^4.2.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "^4.13.1",
    "np": "^5.0.3",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "1.19.1",
    "react-app-polyfill": "^1.0.5",
    "react-dev-utils": "^10.0.0",
    "react-svg-loader": "^3.0.3",
    "resolve": "1.12.2",
    "resolve-url-loader": "3.1.1",
    "run-electron": "^1.0.0",
    "sass-loader": "8.0.0",
    "semver": "6.3.0",
    "style-loader": "1.0.0",
